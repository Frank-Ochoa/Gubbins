int foo := 50000;
int bar := 5;

( int foo, int bar, (int a) -> int fred ) object := rec
(

    int foo := 600,
    int bar := 6,

    (int a) -> int fred := func (int a) -> int
    {
        for i in range 20
        {
            if(i = 9 | i = 5)
            {
                return foo;
            }
        }

        return 0;
    }

);

print object.fred(0);
